	<%! static int count = 10; %>
	
	<%	
		//이거를 스크립틀릿이라고 합니다. 스크립트 태그 중 하나.
		//이 구간은 자바문법이 적용된다.
		for(int i=0;i<5;i++) {
	%>
			out.println("<p>count " + i + "</p>"); <br>
			<p>count <%= i %></p>
	<%
		}
		count = 500;
	%>
	
	
	

</body>
